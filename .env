# Environment variables
# Guardian Eagle Dashboard Environment Variables

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000/api
REACT_APP_WS_URL=ws://localhost:8000/ws

# Map Configuration (if using custom tiles)
REACT_APP_MAPS_API_KEY=your_maps_api_key_here

# Environment
REACT_APP_ENVIRONMENT=development

# Features Flags
REACT_APP_ENABLE_GEOLOCATION=true
REACT_APP_ENABLE_WEBSOCKETS=true
REACT_APP_ENABLE_MOCK_DATA=true

# Debug Settings
REACT_APP_DEBUG_WEBSOCKETS=false
REACT_APP_DEBUG_API_CALLS=false

# Security
REACT_APP_SESSION_TIMEOUT=3600000
REACT_APP_ENABLE_HTTPS_ONLY=false

# Application Settings
REACT_APP_DEFAULT_MAP_CENTER_LAT=40.7128
REACT_APP_DEFAULT_MAP_CENTER_LNG=-74.0060
REACT_APP_DEFAULT_MAP_ZOOM=13

# Notification Settings
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_NOTIFICATION_TIMEOUT=5000
